<Window	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cf="clr-namespace:CountryFlag;assembly=CountryFlag"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="MainWindow"
        x:Name="Window"
        Title="Flags"
        Width="358" Height="280" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" mc:Ignorable="d">
    <Window.Background>
        <RadialGradientBrush Center="0.152,0.257" GradientOrigin="0.085,0.177" RadiusX="1.121" RadiusY="1.121">
            <GradientStop Color="#FFC2CDD0" Offset="0"/>
            <GradientStop Color="#FFAEC7CE" Offset="1"/>
            <GradientStop Color="#FF596E74" Offset="0.595"/>
        </RadialGradientBrush>
    </Window.Background>

    <Grid x:Name="LayoutRoot">
        <WrapPanel Margin="5">
            <cf:CountryFlag Code="BT" Margin="5"/>
            <cf:CountryFlag Code="AF" Margin="5"/>
            <cf:CountryFlag Code="AO" Margin="5"/>
            <cf:CountryFlag Code="BB" Margin="5"/>
            <cf:CountryFlag Code="KE" Margin="5"/>
            <cf:CountryFlag Code="BR" Margin="5"/>
            <cf:CountryFlag Code="EG" Margin="5"/>
            <cf:CountryFlag Code="RS" Margin="5"/>
            <cf:CountryFlag Code="SZ" Margin="5"/>
        </WrapPanel>
    </Grid>
</Window>